opied_from_pointer":{"id":"6ff0805d-a9fb-4a5e-a707-fe9254ace39d","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535474,"last_edited_time":1678272535474,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"6ff0805d-a9fb-4a5e-a707-fe9254ace39d","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"1edab5ff-b415-41ab-a064-7cec05db9d7d":{"role":"comment_only","value":{"id":"1edab5ff-b415-41ab-a064-7cec05db9d7d","version":1,"type":"sub_sub_header","properties":{"title":[["demo"]]},"format":{"copied_from_pointer":{"id":"b6c609b6-0761-4a46-9425-ffe382a977f2","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"b6c609b6-0761-4a46-9425-ffe382a977f2","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"e4af3c92-e20c-4e85-81c8-61efdc8f807d":{"role":"comment_only","value":{"id":"e4af3c92-e20c-4e85-81c8-61efdc8f807d","version":1,"type":"code","properties":{"title":[["function throttle(fn, wait){\n  let pervious = 0\n\n  return function (...args) {\n    let now = Date.now()\n    if(now - pervious > wait) {\n      pervious = now\n      fn.apply(this, args)\n    }\n  }\n}"]],"language":[["Plain Text"]]},"format":{"code_wrap":true,"copied_from_pointer":{"id":"b69a41ad-e144-4963-a6f7-348a74b43a55","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"b69a41ad-e144-4963-a6f7-348a74b43a55","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"56fcf0d7-f2f1-445c-a597-f395e0f15b9d":{"role":"comment_only","value":{"id":"56fcf0d7-f2f1-445c-a597-f395e0f15b9d","version":1,"type":"sub_sub_header","properties":{"title":[["underscore源码分析"]]},"format":{"copied_from_pointer":{"id":"21631737-34ce-4192-860e-de1c8c340e18","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535474,"last_edited_time":1678272535474,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"21631737-34ce-4192-860e-de1c8c340e18","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"af988a3b-2aa4-4bd2-9e84-f5086c5c1514":{"role":"comment_only","value":{"id":"af988a3b-2aa4-4bd2-9e84-f5086c5c1514","version":1,"type":"code","properties":{"title":[[" /* options的默认值\n  *  表示首次调用返回值方法时，会马上调用func；否则仅会记录当前时刻，当第二次调用的时间间隔超过wait时，才调用func。\n  *  options.leading = true;\n  *  表示当调用方法时，未到达wait指定的时间间隔，则启动计时器延迟调用func函数，若后续在既未达到wait指定的时间间隔和func函数又未被调用的情况下调用返回值方法，则被调用请求将被丢弃。\n  *  options.trailing = true;\n  *  注意：当options.trailing = false时，效果与上面的简单实现效果相同\n  */\n_.throttle = function(func, wait, options) {\n    var timeout, context, args, result;\n    var previous = 0;\n    if (!options) options = {};\n\n    var later = function() {\n      previous = options.leading === false ? 0 : _.now();\n      timeout = null;\n      result = func.apply(context, args);\n      if (!timeout) context = args = null;\n    };\n\n    var throttled = function() {\n      var now = _.now();\n      if (!previous && options.leading === false) previous = now; // 默认为时开头就调用一次的， Date.now() - previous (0) > wait\n      var remaining = wait - (now - previous);\n      context = this;\n      args = arguments;\n      if (remaining <= 0 || remaining > wait) { // 当间隔大于wait 或者 当前时间小于上一个时间(！！！客户端人为的更改系统时间后，马上执行了func函数)\n        if (timeout) {\n          clearTimeout(timeout);\n          timeout = null;\n        }\n        previous = now;\n        result = func.apply(context, args);\n\n        if (!timeout) context = args = null;\n      } else if (!timeout && options.trailing !== false) { // 无timeout和 允许结尾时调用一次函数\n        timeout = setTimeout(later, remaining);\n      }\n      return result;\n    };\n\n    throttled.cancel = function() {\n      clearTimeout(timeout);\n      previous = 0;\n      timeout = context = args = null;\n    };\n\n    return throttled;\n  };"]],"language":[["Plain Text"]]},"format":{"code_wrap":true,"copied_from_pointer":{"id":"a7086d9b-8406-497a-8c34-855887ece9ad","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"a7086d9b-8406-497a-8c34-855887ece9ad","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"76529fef-cd62-4f42-a324-3dcbc7704362":{"role":"comment_only","value":{"id":"76529fef-cd62-4f42-a324-3dcbc7704362","version":1,"type":"sub_sub_header","properties":{"title":[["防抖"]]},"format":{"copied_from_pointer":{"id":"94967f11-dd6c-462c-a9cf-b0ed7d0fe2f4","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"94967f11-dd6c-462c-a9cf-b0ed7d0fe2f4","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"a7f1eb74-8f26-4445-9466-d5f98ba01a23":{"role":"comment_only","value":{"id":"a7f1eb74-8f26-4445-9466-d5f98ba01a23","version":1,"type":"sub_sub_header","properties":{"title":[["介绍"]]},"format":{"copied_from_pointer":{"id":"4e1ea919-a722-4b89-8ffa-9b12a401364d","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535474,"last_edited_time":1678272535474,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"4e1ea919-a722-4b89-8ffa-9b12a401364d","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"a44b7f59-55e7-42eb-b36a-7a3a93cb8d30":{"role":"comment_only","value":{"id":"a44b7f59-55e7-42eb-b36a-7a3a93cb8d30","version":1,"type":"text","properties":{"title":[["如果用手指一直按住一个弹簧，它将不会弹起直到你松手为止。也就是说当调用动作n毫秒后，才会执行该动作，若在这n毫秒内又调用此动作则将重新计算执行时间。"]]},"format":{"copied_from_pointer":{"id":"f46cb67d-0298-415b-b00d-3aca9b5fb07c","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"f46cb67d-0298-415b-b00d-3aca9b5fb07c","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"524d6a34-005f-4950-b876-085c2dbf023c":{"role":"comment_only","value":{"id":"524d6a34-005f-4950-b876-085c2dbf023c","version":1,"type":"sub_sub_header","properties":{"title":[["简单demo"]]},"format":{"copied_from_pointer":{"id":"b1939222-30c7-4dd6-8b15-583eea2b6aef","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"b1939222-30c7-4dd6-8b15-583eea2b6aef","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"c2abab18-4771-4c5f-a207-d5b2d0eec586":{"role":"comment_only","value":{"id":"c2abab18-4771-4c5f-a207-d5b2d0eec586","version":1,"type":"code","properties":{"title":[["function debouce (fn, wait, immediate) {\n  let timer = null\n\n  return function(...args){\n    <!-- 清除原来的延时器 -->\n    if(tiemr) clearTimeout(timer)\n\n    <!-- 第一次调用 -->\n    if (immediate && !tiemr){\n      fn.apply(this, args)\n    }\n\n    timer = setTimeout(function() {\n      fn.apply(this, args)\n    }, wait)\n\n  }\n}"]],"language":[["Plain Text"]]},"format":{"code_wrap":true,"copied_from_pointer":{"id":"f11bc6ef-dacc-4875-990a-f182acd82904","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"f11bc6ef-dacc-4875-990a-f182acd82904","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"c08029df-3eba-4a41-9ea3-f34e3a6050bf":{"role":"comment_only","value":{"id":"c08029df-3eba-4a41-9ea3-f34e3a6050bf","version":1,"type":"sub_sub_header","properties":{"title":[["加强版的throttle"]]},"format":{"copied_from_pointer":{"id":"c63b7349-1018-4d40-a9f4-724d58586cc9","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"c63b7349-1018-4d40-a9f4-724d58586cc9","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"7d87a943-33d6-4a24-8e31-28b06400526c":{"role":"comment_only","value":{"id":"7d87a943-33d6-4a24-8e31-28b06400526c","version":1,"type":"text","properties":{"title":[["现在考虑一种情况，如果用户的操作非常频繁，不等设置的延迟时间结束就进行下次操作，会频繁的清除计时器并重新生成，所以函数 fn 一直都没办法执行，导致用户操作迟迟得不到响应。"],["有一种思想是将「节流」和「防抖」合二为一，变成加强版的节流函数，关键点在于「 wait 时间内，可以重新生成定时器，但只要 wait 的时间到了，必须给用户一个响应。这种合体思路恰好可以解决上面提出的问题。"]]},"format":{"copied_from_pointer":{"id":"feba532d-36f9-4358-b4d9-6b0993842af0","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"feba532d-36f9-4358-b4d9-6b0993842af0","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"f144a87b-dced-450d-abea-85fa4d96918f":{"role":"comment_only","value":{"id":"f144a87b-dced-450d-abea-85fa4d96918f","version":1,"type":"code","properties":{"title":[["function throttle(fn, wait , immediate) {\n  let pervious = 0\n  let timer = null\n\n  return function(...args) {\n    let now = Date.now()\n\n    if(now - pervious < wait) {\n      if (timer) clearTimeout(timer)\n      if (immediate && !timer) {\n        fn.apply(this, args)\n      }\n      timer = setTimeout(function() {\n        pervious = now\n        fn.apply(this, args)\n      })\n\n    }else{\n      pervious = now\n      fn.apply(this, args)\n    }\n  }\n}"]],"language":[["Plain Text"]]},"format":{"code_wrap":true,"copied_from_pointer":{"id":"e992a332-6ba3-419e-8b4e-ee0b8dcd6fd0","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"e992a332-6ba3-419e-8b4e-ee0b8dcd6fd0","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"2f8628c7-9aad-4584-80cd-74fc551e6dce":{"role":"comment_only","value":{"id":"2f8628c7-9aad-4584-80cd-74fc551e6dce","version":1,"type":"sub_sub_header","properties":{"title":[["underscore源码分析"]]},"format":{"copied_from_pointer":{"id":"77c69421-baae-4f52-a1b2-75ba18ed760a","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535475,"last_edited_time":1678272535475,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"77c69421-baae-4f52-a1b2-75ba18ed760a","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"9ce00233-7698-4d89-8e75-1786747a08dd":{"role":"comment_only","value":{"id":"9ce00233-7698-4d89-8e75-1786747a08dd","version":1,"type":"text","properties":{"title":[["实现原理和demo可以说是一摸一样"]]},"format":{"copied_from_pointer":{"id":"505985a1-ddea-4f70-8aba-7294e661120d","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535474,"last_edited_time":1678272535474,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"505985a1-ddea-4f70-8aba-7294e661120d","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edited_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","space_id":"83c8a87b-49d0-47a1-947d-0dc6f08e4d02"}},"732f57de-b1c7-41b2-a5a2-6819882456f9":{"role":"comment_only","value":{"id":"732f57de-b1c7-41b2-a5a2-6819882456f9","version":1,"type":"code","properties":{"title":[["// 此处的三个参数上文都有解释\n_.debounce = function(func, wait, immediate) {\n  var timer, result;\n\n  // 定时器计时结束后\n  // 1、清空计时器，使之不影响下次连续事件的触发\n  // 2、触发执行 func\n  var later = function(context, args) {\n    timer = null;\n    if (args) result = func.apply(context, args);\n  };\n\n  // 将 debounce 处理结果当作函数返回\n  var debounced = restArguments(function(args) {\n    if (timer) clearTimeout(timer);\n    if (immediate) {\n      // 第一次触发后会设置 timer，\n      // 根据 timer 是否为空可以判断是否是首次触发\n      var callNow = !timer;\n      timer = setTimeout(later, wait);\n      if (callNow) result = func.apply(this, args);\n    } else {\n        // 设置定时器\n      timer = _.delay(later, wait, this, args);\n    }\n\n    return result;\n  });\n\n  // 新增 手动取消\n  debounced.cancel = function() {\n    clearTimeout(timer);\n    timer = null;\n  };\n\n  return debounced;\n};\n\n// 根据给定的毫秒 wait 延迟执行函数 func\n_.delay = restArguments(function(func, wait, args) {\n  return setTimeout(function() {\n    return func.apply(null, args);\n  }, wait);\n});"]],"language":[["Plain Text"]]},"format":{"code_wrap":true,"copied_from_pointer":{"id":"120b7e59-ae17-4fb6-93da-c61d9b469c3a","table":"block","spaceId":"64f32eab-5982-4561-b4f8-3fb986272cf2"}},"created_time":1678272535474,"last_edited_time":1678272535474,"parent_id":"6442db91-36a8-4b6f-89fb-0e8de9a4dafd","parent_table":"block","alive":true,"copied_from":"120b7e59-ae17-4fb6-93da-c61d9b469c3a","created_by_table":"notion_user","created_by_id":"60795f26-efa0-4fd5-b77e-d982ef21c9a3","last_edited_by_table":"notion_user","last_edit